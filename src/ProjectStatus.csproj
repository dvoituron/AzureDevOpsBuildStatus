<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>netcoreapp3.0</TargetFramework>
    <PackAsTool>true</PackAsTool>
    <ToolCommandName>az-devops-status</ToolCommandName>
    <Version>1.0.0</Version>
    <Authors>Denis Voituron</Authors>
    <Company>Denis Voituron</Company>
    <Product>Azure DevOps Project Status</Product>
    <Title>Azure DevOps Project Status</Title>
    <PackageId>Dvoituron.DevOps.ProjectStatus</PackageId>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/dvoituron</PackageProjectUrl>
    <RepositoryUrl>https://github.com/dvoituron</RepositoryUrl>
    <PackageTags>Azure DevOps Build Release Status</PackageTags>
    <Copyright>Copyright 2020 Denis Voituron</Copyright>
    <Description>Use this CLI tool to display all Projects Build Status.</Description>
    <AssemblyVersion>1.0.0.0</AssemblyVersion>
    <FileVersion>1.0.0.0</FileVersion>
    <DocumentationFile></DocumentationFile>
  </PropertyGroup>

</Project>
